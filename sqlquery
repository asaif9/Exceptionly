SELECT team, sum(point) FROM Employee where pt_date BETWEEN '24/04/2021' and '28/04/2021' GROUP BY team,name 
SELECT team, sum(point) FROM Employee where WEEKOFYEAR(pt_date)=WEEKOFYEAR(CURDATE())-1 GROUP BY team,name 
SELECT team, sum(point) FROM Employee where pt_date >= DATEADD(day,-11117, GETDATE()) GROUP BY team,name 


